
config 'system' 'system_0'
	option 'timezone' 'UTC'
	option 'log_enable' '1'
	option 'log_level' '3'
	option 'hostname' 'MSS5004W'

config 'rdate' 'rdate_0'
	option 'interface' 'wan'

config 'led' 'wlan_led'
	option 'name' 'WLAN'
	option 'sysfs' 'rl_ans5004:green:wlan'
	option 'trigger' 'netdev'
	option 'dev' 'wifi0'
	option 'mode' 'link tx rx'
	option 'interval' '300'

config 'led' 'led_system'
	option 'name' 'system'
	option 'sysfs' 'richerlink:green:system'
	option 'trigger' 'timer'
	option 'delayon' '500'
	option 'delayoff' '300'

